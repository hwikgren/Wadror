<table class="table table-striped table-condensed">
  <thead>
    <th>Name</th>
    <th>Average</th>
  </thead>
  <tbody>
    <% top.each do |object| %>
      <tr>
        <td> <%= object.name %></td>
        <td><%= round(object.average_rating) %> </td>
      </tr>
    <% end %>
  </tbody>
</table>
